{
  "description": "AWS CDK ベースの Apache Iceberg データレイクプロジェクト",
  "main": "index.js",
  "name": "iceberg-cdk-app",
  "version": "0.1.0",
  "bin": {
    "iceberg-cdk-app": "bin/iceberg_cdk_app.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "ts-node scripts/clean.ts",
    "clean:aws": "ts-node -e \"import {CleanupManager, CleanupLevel} from './scripts/cleanup-utils'; new CleanupManager().performCleanup(CleanupLevel.AWS_INFRASTRUCTURE)\"",
    "clean:silent": "ts-node scripts/silent-cleanup.ts",
    "clean:full": "ts-node scripts/comprehensive-cleanup.ts",
    "cleanup": "ts-node scripts/comprehensive-cleanup.ts",
    "setup": "node test/setup.js",
    "validate": "node test/validate-setup.js",
    "test": "npm run build && jest",
    "test:sequential": "npm run build && jest --runInBand --testSequencer=./test/jest.sequencer.js",
    "test:coverage": "npm run build && jest --runInBand --testSequencer=./test/jest.sequencer.js --coverage",
    "test:watch": "npm run build && jest --watch",
    "deploy": "npm run build && cdk deploy",
    "deploy:safe": "npm run deploy && npm run drift:verify",
    "deploy-and-setup": "ts-node scripts/deploy-and-setup.ts",
    "destroy": "cdk destroy",
    "drift:detect": "ts-node scripts/drift-detection.ts",
    "drift:verify": "ts-node scripts/post-deploy-verify.ts",
    "demo": "ts-node demo.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.19",
    "aws-cdk": "2.1024.0",
    "dotenv": "^17.2.1",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-athena": "^3.863.0",
    "@aws-sdk/client-glue": "^3.863.0",
    "@aws-sdk/client-s3": "^3.863.0",
    "@aws-sdk/client-sts": "^3.863.0",
    "@aws-sdk/credential-providers": "^3.863.0",
    "@aws-sdk/types": "^3.862.0",
    "aws-cdk-lib": "2.210.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1"
}
